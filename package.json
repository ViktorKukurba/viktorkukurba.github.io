  "private": true,
  "devDependencies": {
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/preset-env": "^7.1.5",
    "bootstrap": "^4.1.3",
    "clean-webpack-plugin": "^1.0.0",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.7.0",
    "esdoc": "^1.1.0",
    "esdoc-ecmascript-proposal-plugin": "^1.0.0",
    "esdoc-jsx-plugin": "^1.0.0",
    "esdoc-standard-plugin": "^1.0.0",
    "eslint": "^5.6.0",
    "eslint-config-react-app": "^3.0.5",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^2.0.0",
    "html-webpack-alter-asset-plugin": "^1.0.0",
    "html-webpack-exclude-assets-plugin": "^0.0.7",
    "html-webpack-plugin": "^3.2.0",
    "identity-obj-proxy": "^3.0.0",
    "install": "^0.12.2",
    "jest": "^23.6.0",
    "jest-css-modules": "*",
    "ncp": "^2.0.0",
    "node-sass": "^4.10.0",
    "prop-types": "^15.6.2",
    "raf-polyfill": "^1.0.0",
    "react-addons-test-utils": "^15.6.2",
    "react-owl-carousel": "^2.3.1",
    "react-popper": "^1.3.0",
    "react-scripts": "^2.1.1",
    "react-scroll": "^1.7.10",
    "reactstrap": "6.5.0",
    "sass-loader": "^7.1.0",
    "tapable": "^1.1.0",
    "upwork-api": "^1.3.0"
  },
  "dependencies": {
    "@babel/core": "^7.1.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "babel-eslint": "9.0.0",
    "express": "^4.16.4",
    "gh-pages": "^2.0.1",
    "jquery": "^3.3.1",
    "react": "^16.6.1",
    "react-dom": "^16.6.1",
    "react-redux": "^5.1.1",
    "react-scripts": "2.1.1",
    "redux": "^4.0.1",
    "redux-thunk": "*",
    "save-dev": "^2.0.0",
    "webpack": "4.19.1",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "3.1.9",
    "whatwg-fetch": "^3.0.0"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(css|sass)$": "identity-obj-proxy",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__tests__/fileMock.js"
    },
    "transform": {
      ".*": "<rootDir>/node_modules/jest-css-modules"
    },
    "browser": true,
    "collectCoverageFrom": [
      "**/src/**/*.{js,jsx}",
      "!**/node_modules/**",
      "!**/coverage/**",
      "!**/dist/**",
      "!**/build/**",
      "!**/doc/**"
    ],
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.jsx?$",
    "setupTestFrameworkScriptFile": "<rootDir>/src/setupTests.js",
    "testPathIgnorePatterns": [
      "fileMock.js"
    ]
  },
  "scripts": {
    "start": "react-scripts start",
    "start:node": "node server.js",
    "start:dev": "webpack-dev-server --mode development --hot --inline",
    "start:prod": "webpack-dev-server --mode production --hot --inline",
    "build": "react-scripts build",
    "build:dev": "webpack --mode development --progress && node scripts/copy_public.js",
    "build:prod": "webpack --mode production -p",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "test": "jest --coverage --env=jsdom --notify",
    "doc": "esdoc",
    "postinstall": "webpack --progress --mode production -p && node scripts/copy_public.js"
  },
  "engines": {
    "node": "9.3.0",
    "npm": "5.6.0"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
